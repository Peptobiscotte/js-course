{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": [
            "error",
            {
                "code": 80,
                "ignoreComments": true
            }
        ]
    }
}